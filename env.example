# GA4 측정 ID (예: G-XXXXXXXXXX)
VITE_GA_MEASUREMENT_ID=


# Gemini 2.0 Flash API Key
VITE_GEMINI_API_KEY=


# 개발 모드 설정
VITE_NODE_ENV=development

# Wishing Peace Map Environment Variables

# ACLED API (무료 회원가입 필요)
VITE_ACLED_EMAIL=your-email@example.com
VITE_ACLED_ACCESS_KEY=your-access-key

# News API (무료 티어 100req/day)
VITE_NEWS_API_KEY=your-news-api-key

# OpenStreetMap 설정
VITE_OSM_TILES_URL=https://tile.openstreetmap.org/{z}/{x}/{y}.png

# 애플리케이션 설정
VITE_APP_URL=http://localhost:3000
VITE_APP_NAME=Wishing Peace Map

# 개발 모드 설정
VITE_DEV_MODE=true
VITE_ENABLE_ANALYTICS=false
